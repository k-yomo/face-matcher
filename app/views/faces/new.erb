<h1>Upload Image</h1>

<form action="/faces" method="post" enctype="multipart/form-data">
  <div class="form-group">
      <label for="self-face">Your Face</label>
      <input type="file" name="face1" id="self-face" class="form-control-file">
  </div>

  <div class="form-group">
      <label for="ideal-face">Your Face</label>
      <input type="file" name="face2" id="ideal-face" class="form-control-file">
  </div>

  <button type="submit" class="btn btn-secondary">Upload images</button>
</form>
